# Variables
set bell-style none
set colored-stats on
set show-all-if-ambiguous on
set show-all-if-unmodified on
set completion-ignore-case on

# Custom key-bindings
"\C-o0": shell-forward-word
# Move to the beginning of next word and delete the previous word
"\C-d": "\C-o0\C-w"
"\C-t": forward-search-history
